{
	"manifest_version": 2,
	"name": "快捷添加 cookie",
	"version": "1.0.1",
	"description": "快捷添加 cookie",
	"icons": {
		"16": "icon/icon.png"
	},
	"background":
	{
		"page": "background.html"
	},
	"browser_action": 
	{
		"default_icon": "icon/icon.png",
		"default_title": "快捷添加 cookie",
		"default_popup": "popup.html"
	},
	"permissions":
	[
		"tabs",
		"storage"
	],
	
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}